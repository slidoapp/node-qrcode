{
  "name": "@slidoapp/qrcode",
  "description": "Generate Quick Response (QR) codes using JavaScript in a browser, server side code, or using a command line tool.",
  "version": "2.0.0-preview2",
  "author": "Ryan Day <soldair@gmail.com>",
  "contributors": [
    "Vincenzo Greco <greco.vincenzo@gmail.com>",
    "Linus Unneb√§ck <linus@folkdatorn.se>",
    "Jozef Izso <izso@cisco.com>"
  ],
  "keywords": [
    "qr",
    "code",
    "canvas",
    "qrcode"
  ],
  "main": "./lib/index.js",
  "browser": {
    "./lib/index.js": "./lib/browser.js",
    "fs": false
  },
  "files": [
    "bin",
    "build",
    "lib",
    "helper"
  ],
  "homepage": "https://github.com/slidoapp/node-qrcode",
  "license": "MIT",
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "test": "node --throw-deprecation test.js",
    "tap-junit-report": "tap replay -R junit --coverage-report=none --allow-empty-coverage --allow-incomplete-coverage 'test/**/*.test.js' > test-results.xml",
    "build": "rollup -c",
    "prepublish": "npm run build",
    "browser": "node examples/clientsideserver.js"
  },
  "bin": {
    "qrcode": "./bin/qrcode"
  },
  "dependencies": {
    "dijkstrajs": "^1.0.3",
    "pngjs": "^7.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "browserify": "^16.5.2",
    "canvas": "^2.11.2",
    "canvasutil": "0.0.4",
    "colors": "^1.4.0",
    "express": "^4.21.1",
    "htmlparser2": "^4.1.0",
    "rollup": "^2.79.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.1",
    "sinon": "^9.2.4",
    "standard": "^16.0.4",
    "tap": "^20.0.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slidoapp/node-qrcode.git"
  },
  "engines": {
    "node": ">=18"
  },
  "standard": {
    "ignore": [
      "build/",
      "examples/vendors/",
      "lib/core/regex.js"
    ]
  }
}
